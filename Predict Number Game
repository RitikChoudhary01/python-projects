import random



n = random.randint(1,100)
attempt = 1
a = -1

while(a != n):
    a = int(input("enter the number :  " ))
    if( a>n ):
        print("too high. please enter low number..")
        attempt += 1


    elif( a<n ):
        print("too low. please enter high number..")
        attempt += 1

print(f"you successfully guess number is {n}. in {attempt} attempt")
